{
  "name": "revolution/laravel-laminas-form",
  "description": "Laravel Laminas Form",
  "keywords": [
    "laminas-form",
    "form",
    "laravel"
  ],
  "license": "MIT",
  "require": {
    "php": "^7.2",
    "laminas/laminas-escaper": "^2.6",
    "laminas/laminas-form": "^2.12",
    "laminas/laminas-i18n": "^2.9",
    "laminas/laminas-servicemanager": "^3.3",
    "laminas/laminas-view": "^2.10",
    "illuminate/support": "*",
    "illuminate/container": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0",
    "mockery/mockery": "^1.0",
    "orchestra/testbench": "^4.0"
  },
  "autoload": {
    "psr-4": {
      "Revolution\\LaminasForm\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "kawax",
      "email": "kawaxbiz@gmail.com"
    }
  ],
  "extra": {
    "laravel": {
      "providers": [
        "Revolution\\LaminasForm\\Providers\\LaminasFormServiceProvider"
      ]
    }
  }
}
